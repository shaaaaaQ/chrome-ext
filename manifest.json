{
    "name": "My Chrome Extensions",
    "version": "0.1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search*"
            ],
            "css": [
                "google-search/style.css"
            ]
        },
        {
            "matches": [
                "https://tweetdeck.twitter.com/*"
            ],
            "css": [
                "tweetdeck/style.css"
            ]
        }
    ]
}